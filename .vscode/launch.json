    "version": "0.2.0",
    "configurations": [
        {
            "type": "node",
            "name": "Launch Client",
            "request": "launch",
            "skipFiles": [
                "<node_internals>/**"
            ],
            "program": "${workspaceFolder}/src/client/Client.ts",
            "outFiles": [
                "${workspaceFolder}/lib/**/*.js"
            ]
        },
        {
            "name": "Express Server",
            "type": "node",
            "request": "launch",
            "skipFiles": [
                "<node_internals>/**"
            ],
            "program": "${workspaceFolder}/src/express-server/LocalServer.ts",
            "outFiles": [
                "${workspaceFolder}/lib/**/*.js"
            ]
        },   
    ]
}